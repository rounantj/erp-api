{
  "name": "reboot-erp-api",
  "version": "1.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "copy-files": "copyfiles -u 1 src/**/*.html dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --config ./test/jest.json",
    "test:ci": "jest --config ./test/jest.json --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": " ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/api/config/typeorm.config.ts",
    "m:generate": "npm run build && npm run typeorm migration:generate -n ./src/migrations/postgres/$npm_config_name",
    "mw:generate": "npm run build && npm run typeorm migration:generate -n ./src/migrations/postgres/%npm_config_name%",
    "m:create": "typeorm migration:create ./src/migrations/postgres/$npm_config_name",
    "mw:create": "typeorm migration:create ./src/migrations/postgres/%npm_config_name%",
    "m:run": "npm run typeorm migration:run",
    "mw:run": "npm run typeorm migration:run",
    "m:revert": "npm run typeorm migration:revert",
    "mw:revert": "npm run typeorm migration:revert",
    "create-token": "tsx src/scripts/create-token.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.484.0",
    "@aws-sdk/client-sts": "^3.507.0",
    "@langchain/community": "^0.0.19",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.1",
    "@types/pdf-parse": "^1.1.4",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "google-auth-library": "^8.9.0",
    "langchain": "^0.1.5",
    "mammoth": "^1.6.0",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "nestjs-newrelic": "^0.0.2",
    "newrelic": "^9.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pgvector": "^0.1.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "text-encoding": "^0.7.0",
    "typeorm": "^0.3.19",
    "xlsx": "^0.18.5",
    "xlsx-extract": "^0.1.3"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.8",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^9.4.0",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.0.11",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "tsx": "^3.12.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
